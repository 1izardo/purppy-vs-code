			"settings": {
				"foreground": "#8ab8c5"
			}
		},
		{
			"scope": ["keyword.operator"],
			"settings": {
				"foreground": "#b5c292"
			}
		},
		{
			"scope": ["comment.block", "comment.line"],
			"settings": {
